#!/usr/bin/perl

use strict;
use warnings;
use Crypt::Mode::ECB;

my $data = '37412C478E0FBEAB';
my $key = '42cc19ac-7922-40';

my $ecb = Crypt::Mode::ECB->new('AES');
my $ciphertext =$ecb->encrypt($data, $key);
$ciphertext =~ s/(.)/sprintf("%x",ord($1))/eg;
print substr(uc($ciphertext),0,32),"\n";

